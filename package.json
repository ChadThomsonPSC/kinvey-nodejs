{
  "name": "kinvey-node-sdk",
  "version": "3.0.0",
  "description": "Kinvey JavaScript SDK for NodeJS applications.",
  "homepage": "http://www.kinvey.com",
  "bugs": {
    "url": "http://support.kinvey.com",
    "email": "support@kinvey.com"
  },
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "https://raw.github.com/Kinvey/kinvey-node-sdk/master/LICENSE.txt"
    }
  ],
  "author": "Kinvey, Inc.",
  "main": "./dist/index.js",
  "contributors": [
    "Thomas Conner <thomas@kinvey.com>"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:Kinvey/node-sdk"
  },
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha --compilers js:babel-core/register -r babel-polyfill -s 100 --recursive test",
    "test:watch": "./node_modules/mocha/bin/mocha -w --compilers js:babel-core/register -r babel-polyfill -s 100 --recursive test",
    "test:jenkins": "istanbul cover _mocha -- --reporter tap --compilers js:babel-core/register -r babel-polyfill -s 100 --recursive test > test.tap && istanbul report clover"
  },
  "dependencies": {
    "kinvey-javascript-sdk-core": "3.0.0-beta.30",
    "lodash": "^4.3.0",
    "request": "^2.69.0"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.0.2",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.7.2",
    "del": "^2.2.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.2.3",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-bump": "^2.2.0",
    "gulp-eslint": "^2.0.0",
    "gulp-git": "^1.7.1",
    "gulp-if": "^2.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-prompt": "^0.2.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-tag-version": "^1.3.0",
    "gulp-util": "^3.0.7",
    "istanbul": "^0.4.4",
    "mocha": "^2.5.3",
    "nock": "^8.0.0",
    "run-sequence": "^1.1.5",
    "semver-regex": "^1.0.0",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "yargs": "^4.7.1"
  },
  "engines": {
    "node": ">=4.0"
  },
  "keywords": [
    "Kinvey",
    "NodeJS"
  ],
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
      "comma-dangle": "off",
      "func-names": "off",
      "id-length": "off",
      "max-len": [
        "warn",
        {
          "code": 120,
          "tabWidth": 2,
          "ignoreComments": true,
          "ignoreTrailingComments": true
        }
      ],
      "no-console": "error",
      "no-loop-func": "warn",
      "no-param-reassign": "off",
      "no-shadow": "off",
      "no-underscore-dangle": "off",
      "object-shorthand": [
        "error",
        "methods"
      ],
      "space-before-function-paren": "off"
    },
    "env": {
      "browser": true,
      "es6": true,
      "mocha": true,
      "node": true
    }
  }
}
